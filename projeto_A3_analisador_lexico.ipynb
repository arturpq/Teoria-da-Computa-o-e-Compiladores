            "\n",
            "public static void main args[] {\n",
            "  int \$int\$_teste = 1 + 2 * 3;\n",
            "  double dbl = 1. * .5d;\n",
            "  Objeto objeto = new Objeto();\n",
            "  String string_teste = \"literal, literal!\";\n",
            "  boolean bool = true;\n",
            "  char chr = 'a'\n",
            "  char chr2 = '/n'\n",
            "  \n",
            "    \n",
            "  if (\$int\$_teste > 0 && (int_teste <= 2 || boolean)) {\n",
